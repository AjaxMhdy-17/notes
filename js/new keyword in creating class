
class Person{
	constructor(fname , lname , salary){
		this.fname = fname ;
		this.lname = lname ;
		this.salary = salary 
	}
}

const p1 = new Person('hero' , 'name2' , 20000)
console.log(p1);


const p2 = new Person('friendly' , 'name2' , 25000)
console.log(p2);




function callBackExample(name , age , task()){
	console.log('name : ' + name)
	console.log('age : ' + age)
	task() //call back function is passing a 
			// function as a parameter and 
			///call it inside the function 
}


function Stricker(){
	console.log('He is a Stricker')
}

function Defender(){
	console.log('He is Defender')
}

function GoalKeeper(){
	console.log('He is GoalKeeper')
}

callBackExample('Meem' , 28 , Stricker)
callBackExample('Mhdy' , 27 , Defender )
callBackExample('Ajax' , 27 , GoalKeeper )
